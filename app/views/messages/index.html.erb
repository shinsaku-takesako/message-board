
<div class="panel panel-default">
    <div class="panel-heading">
        Message一覧
    </div>

    <div class="panel-body">
        <% @messages.each do |message| %>
            <span class="label label-info"><%= message.name %></span>
            <%= message.body %>
            <br>
        <% end %>
    </div>
</div>

<% #部分テンプレート読込 %>

<%= render 'form' %>

</div>